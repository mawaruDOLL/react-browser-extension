{
	"name": "React Browser Extension",
	"version": "0.1.0",
	"description": "A sample React browser extension",
	"manifest_version": 2,
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "React Browser Extension"
	},
	"options_ui": {
		"page": "options.html",
		"chrome_style": true
	}
}
