{
  "name": "react-browser-extension",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "SET NODE_ENV=development&& webpack --config ./webpack.config.js --watch",
    "build": "SET NODE_ENV=production&& webpack --config ./webpack.config.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/node": "^11.11.3",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.29.6",
    "webpack-chrome-extension-reloader": "^1.2.0",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "chrome-extension-async": "^3.3.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "typescript": "^3.3.3333"
  }
}
